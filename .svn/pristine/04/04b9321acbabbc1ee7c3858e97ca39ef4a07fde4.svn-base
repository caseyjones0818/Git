<ion-header>
  <ion-navbar color="cangzhen">
    <ion-title>顾问</ion-title>
    <ion-buttons end (click)="fa()">
      <button ion-button icon-only>
        <p>提交</p>
      </button>
    </ion-buttons>
  </ion-navbar>
</ion-header>

<ion-content>
  <ion-item>
    <ion-label color="primary">联系人：</ion-label>
    <ion-input placeholder="输入联系人" maxlength="15" [(ngModel)]="contact"></ion-input>
  </ion-item>
  <ion-item>
    <ion-label color="primary">联系方式：</ion-label>
    <ion-input placeholder="手机、邮箱、QQ、微信" [(ngModel)]="phone"></ion-input>
  </ion-item>
  <ion-item>
    <ion-label color="primary">我的诉求：</ion-label>
    <ion-textarea placeholder="诉求越详细申请成功的几率越大" [(ngModel)]="content"></ion-textarea>
  </ion-item>
</ion-content>
